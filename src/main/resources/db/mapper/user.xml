<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">
    <select id="getAllUsers" parameterType="string" resultType="com.ksbyun.study.demo.user.model.UserVO">
        SELECT * FROM t_user
        WHERE id = #{id};
    </select>
</mapper>